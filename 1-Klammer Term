G={T,N,V,P}
T={1,2,3,4,5,6,7,8,9,0,(,),+,-,*,/}
N={A,B,C,D,E}
V={1,2,3,4,5,6,7,8,9,0,(,),+,-,*,/,A,B,C,D,E}

P={	S→A,		Bsp.: 	-3*(4/5)
	S→-A,		2.:	S	→-A
	A→ABA,		3.:	-A	→-ABA
	A→D,		4.:	-ABA	→-DBA
	A→(C),		5.:	-DBA	→-DB(C)
	A→(-C),		8.:	-DB(C)	→-DB(CBC)
	B→+,-,*,/,	9.:	-DB(CBC)→-DB(DBC)
	C→(CBC),	9.:	-DB(DBC)→-DB(DBD)
	C→D,		7.:	-DB(DBD)→-D*(DBD)
	D→0,		7.:	-D*(DBD)→-D*(D/D)
	D→1-9E,		12.:	-D*(D/D)→-3*(D/D)
	D→1-9,		12.:	-3*(D/D)→-3*(4/D)
	E→0-9E,		12.:	-3*(4/D)→-3*(4/5)
	E→0-9}
